ti kurioje rodytu kokius projektus esu sukures ir prie kuriu esu priskirtas\r\n            </Project.ProjectHeader>\r\n            {!projects\r\n              ? \"\"\r\n              : Object.keys(projects).map((el) => {\r\n                  return (\r\n                    <Project.ProjectCard key={projects[el].projectId}>\r\n                      <img src={userImg} alt=\"user\" />\r\n                      <Project.Wrapper direction=\"column\">\r\n                        <div>\r\n                          <h1\r\n                            onClick={() =>\r\n                              projectRedirectHandler(projects[el], el)\r\n                            }\r\n                          >\r\n                            {projects[el].title}\r\n                          </h1>\r\n                        </div>\r\n                        <p>Created: {projects[el].createdAt}</p>\r\n                        <p>Owner: {projects[el].projectAuthor}</p>\r\n                      </Project.Wrapper>\r\n\r\n                      <Project.Card>\r\n                        <Button\r\n                          onClick={() =>\r\n                            projectRedirectHandler(projects[el], el)\r\n                          }\r\n                          transparent\r\n                        >\r\n                          Overview\r\n                        </Button>\r\n                      </Project.Card>\r\n                      <Project.Card>\r\n                        <h2>Issues</h2>\r\n                        <span>02</span>\r\n                      </Project.Card>\r\n                      <Project.Card>\r\n                        <h2>Employees</h2>\r\n                        <span>02</span>\r\n                      </Project.Card>\r\n                    </Project.ProjectCard>\r\n                  );\r\n                })}\r\n          </Project.Main>\r\n        </Project.Body>\r\n      </Project>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default connect(null, {\r\n  showModal,\r\n  saveCurrentProject,\r\n})(ProjectsPage);\r\n","C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\ProjectSummary\\ProjectSummary.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\Redux\\Reducers\\modalReducer.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\Redux\\Reducers\\authReducer.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Modal\\index.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Projects\\index.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Button\\index.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Dropzone\\Dropzone.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Button\\styles\\button.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Projects\\styles\\projects.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\pages\\Members.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Members\\Members.js",["219","220"],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\MemberCard\\MemberCard.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\helper\\GetCurrentProject.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\containers\\ProjectsDashboard.js",["221"],"import React, { useState } from \"react\";\r\nimport { Dashboard } from \"../components\";\r\nimport logo from \"../images/logo_main.png\";\r\nimport {useSelector} from 'react-redux';\r\n\r\nconst DashboardContainer = () => {\r\n  const [openDashboad, setOpenDashboard] = useState(false);\r\n  const {displayName, email} = useSelector(state => state.firebase.auth);\r\n\r\n  return (\r\n    <Dashboard show={openDashboad}>\r\n      <Dashboard.Summary onClick={() => setOpenDashboard(!openDashboad)}>\r