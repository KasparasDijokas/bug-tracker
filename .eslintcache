\r\n\r\n  return (\r\n    <div style={{display: 'flex'}}>\r\n    <ProjectsDashboard/>\r\n      <Project>\r\n        <Project.Body>\r\n          <Project.Header>\r\n            <img src={user} alt=\"user\" />\r\n            <Project.Wrapper direction=\"column\">\r\n              <h1>{displayName}</h1>\r\n              <p>Registration date: {registrationDate.toLocaleDateString()}</p>\r\n              <p>Last Sign In at: {lastLoginDate.toLocaleString()}</p>\r\n            </Project.Wrapper>\r\n            <Project.Card>\r\n              <h2>My Projects</h2>\r\n              <span>{projects ? Object.keys(projects).length : \"0\"}</span>\r\n            </Project.Card>\r\n            <Project.Card>\r\n              <h2>Employees</h2>\r\n              <span>01</span>\r\n            </Project.Card>\r\n          </Project.Header>\r\n          <Project.Main>\r\n            <Project.ProjectHeader>\r\n              <h2>My Projects</h2>\r\n              <Button onClick={() => props.showModal()}>\r\n                Create New Project\r\n              </Button>\r\n            </Project.ProjectHeader>\r\n            {!projects\r\n              ? \"\"\r\n              : Object.keys(projects).map((el) => {\r\n                  return (\r\n                    <Project.ProjectCard key={projects[el].projectId}>\r\n                      <img src={user} alt=\"user\" />\r\n                      <Project.Wrapper direction=\"column\">\r\n                        <div>\r\n                          <h1\r\n                            onClick={() =>\r\n                              projectRedirectHandler(projects[el], el)\r\n                            }\r\n                          >\r\n                            {projects[el].title}\r\n                          </h1>\r\n                        </div>\r\n                        <p>Created: {projects[el].createdAt}</p>\r\n                        <p>Owner: {projects[el].projectAuthor}</p>\r\n                      </Project.Wrapper>\r\n\r\n                      <Project.Card>\r\n                        <Button\r\n                          onClick={() =>\r\n                            projectRedirectHandler(projects[el], el)\r\n                          }\r\n                          transparent\r\n                        >\r\n                          Overview\r\n                        </Button>\r\n                      </Project.Card>\r\n                      <Project.Card>\r\n                        <h2>Issues</h2>\r\n                        <span>02</span>\r\n                      </Project.Card>\r\n                      <Project.Card>\r\n                        <h2>Employees</h2>\r\n                        <span>02</span>\r\n                      </Project.Card>\r\n                    </Project.ProjectCard>\r\n                  );\r\n                })}\r\n          </Project.Main>\r\n        </Project.Body>\r\n      </Project>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default connect(null, {\r\n  showModal,\r\n  saveCurrentProject,\r\n})(ProjectsPage);\r\n","C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\ProjectSummary\\ProjectSummary.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\Redux\\Reducers\\modalReducer.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\Redux\\Reducers\\authReducer.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Modal\\index.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Projects\\index.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Button\\index.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Dropzone\\Dropzone.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Button\\styles\\button.js",[],"C:\\Users\\bob\\Desktop\\work\\REACT\\github\\bug-tracker\\src\\components\\Projects\\styles\\projects.js",[],"C:\\Users\\bob\\Desktop\\wor